{
  "name": "Houdini worklet lab",
  "version": "1.0.0",
  "description": "Houdini playground",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "gulp dev",
    "dev:project": "gulp dev",
    "dev:all": "gulp devAll",
    "build": "gulp build",
    "build:project": "gulp build",
    "build:all": "gulp buildAll",
    "clean": "gulp clean",
    "scaffold": "gulp scaffold",
    "list:projects": "gulp listProjects",
    "chat:new": "powershell -ExecutionPolicy Bypass -File scripts/chat/prepare-chat-instance.ps1 -PrepareMcp",
    "chat:new:full": "powershell -ExecutionPolicy Bypass -File scripts/chat/prepare-chat-instance.ps1 -InstallRootDeps -InstallMcpDeps -PrepareMcp",
    "chat:new:quick": "powershell -ExecutionPolicy Bypass -File scripts/chat/prepare-chat-instance.ps1 -SkipBuild -PrepareMcp",
    "chat:briefing": "powershell -ExecutionPolicy Bypass -File scripts/chat/new-chat-briefing.ps1 -OutputPath .agency/chat/latest-briefing.md",
    "mcp:prep": "powershell -ExecutionPolicy Bypass -File scripts/chat/prepare-mcp.ps1",
    "mcp:start:filesystem": "cmd /c npm --prefix mcp run start:filesystem",
    "mcp:start:memory": "cmd /c npm --prefix mcp run start:memory",
    "mcp:start:github:legacy": "cmd /c npm --prefix mcp run start:github:legacy",
    "mcp:start:github:modern:docker": "cmd /c npm --prefix mcp run start:github:modern:docker",
    "skills:list": "powershell -ExecutionPolicy Bypass -Command \"Get-ChildItem skills -Directory | Select-Object -ExpandProperty Name\"",
    "skills:validate": "powershell -ExecutionPolicy Bypass -File scripts/chat/validate-skills.ps1",
    "remote:panel": "node remote-console/server.mjs",
    "remote:panel:lan": "node remote-console/server.mjs --host=0.0.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "del": "^7.0.0",
    "gulp": "^5.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-cli": "^3.1.0",
    "gulp-concat": "^2.6.1",
    "gulp-dart-sass": "^1.1.0",
    "gulp-livereload": "^4.0.2",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "sass": "^1.63.0",
    "sharp": "^0.32.0",
    "through2": "^4.0.2"
  },
  "dependencies": {
    "gsap": "^3.14.2"
  }
}
